{
  "name": "ac_observator_dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "npm": "use yarn instead",
    "yarn": "4.5.3"
  },
  "packageManager": "yarn@4.5.3",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "biome lint src/",
    "lint-fix": "biome lint --fix src/",
    "lint:style": "stylelint src/**/*.{css,scss,vue}",
    "format": "biome format src/",
    "format-fix": "biome format --fix src/",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{css,scss,vue}": "stylelint --fix",
    "*": "prettier -w -u"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@mdi/font": "^7.4.47",
    "@nanase/alnilam": "https://github.com/nanase/alnilam.git#0.5.3",
    "@types/async-lock": "^1.4.2",
    "@types/node": "^22.10.2",
    "@types/web-bluetooth": "^0.0.20",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vue-flow/core": "^1.41.6",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/core": "^12.0.0",
    "async-lock": "^1.4.1",
    "chart.js": "^4.4.7",
    "chartjs-adapter-dayjs-4": "1.2.3",
    "chartjs-plugin-annotation": "^3.1.0",
    "dayjs": "^1.11.13",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.3.0",
    "postcss": "^8.4.49",
    "postcss-html": "^1.7.0",
    "sass": "^1.83.0",
    "stylelint": "^16.12.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "5.7.2",
    "unplugin-vue-macros": "^2.13.6",
    "vite": "^6.0.5",
    "vite-plugin-html-inject": "^1.1.2",
    "vite-plugin-webfont-dl": "^3.10.3",
    "vitest": "^2.1.8",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.1.10",
    "vuetify": "^3.7.6"
  }
}
